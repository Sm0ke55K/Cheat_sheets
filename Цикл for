async private void button2_Click(object sender, EventArgs e)
        {
            for (int i = 0; i < 10; i++, File.Create($@"C:\Users\Красотка\Desktop\321\{i}.txt")) // можно добавить несколько действий в цикл for (в его итератор)
            {
                await Task.Delay(1000); // Задержка выполнения
            }
        }

